{
    "compilerOptions": {
      "esModuleInterop": true,
      "target": "ES2020",
      "module": "ES2020",              
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": "./",
      "allowJs": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  